digraph {
    // viridis1 = "#440154";
    // viridis2 = "#31688e";
    // viridis3 = "#35b779";

    fontname = "lato";
    fontsize = "10";
    bgcolor = transparent;
    size = "13.33,7.5";
    dpi = 300;
    rankdir = LR; // Set the layout direction to left-to-right
    compound = true;
    splines = true;

    node [
        fontname = "lato";
        fontsize = "10";
        labelloc = "b";
        imagepos = "tc";
        shape = solid;
        color=none;
        fixedsize = true;
        height = 1.3;
        width = 1;
        imagescale = true;
    ];

    edge [        fontname = "lato";
        fontsize = "10";
]

    // Users
    bpapm [
        label = "BPA PM";
        image = "resources/png/7558197-512.png";
    ];
    lab [
        label = "Sequencing\nlab";
        image = "resources/png/7558264-512.png";

    ];
    researcher [
        label = "Researcher";
        image = "resources/png/7558198-512.png";

    ];


    // BPA data portal
 
    subgraph cluster_dp {
        label = "BioPlatforms";
        compound = true;
        style=solid;
        color=none;
        subgraph cluster_dp1 {
            label="";
            style="dashed,rounded";
            color="#440154"            
        bpadb [
            label = "Data Portal";
            image = "resources/png/ckan-dpg.png";
            imagepos = "c";
            fixedsize = true;
            height = 0.8;
            width = 0.8;

            ];
        }
    


    }

    // AToL data management
    subgraph cluster_dm {
        label = "AToL\ndata mapper";
        compound = true;
        style=solid;
        color=none;
        subgraph cluster_dm1{
            label="";
            style="dashed,rounded";
            color="#35b779"            
        legacy [
                label = "One-off\ntransformation";
                image = "resources/png/7455241-512.png";
            ];
            new [
                label = "Automated\ntransformation";
                image = "resources/png/7455241-512.png";
            ];
        }
    }

    // Data broker
    subgraph cluster_broker {
        compound = true;
            label="";
            style="dashed,rounded";
            color="#35b779"            

        broker [
            label = "Data\nbroker";
                            image = "resources/png/7626477-512.png";

        ];
    }

    // External DBs
    subgraph cluster_ena {
        label = "INSDC\ndatabases";
        compound = true;
        style=solid;
        color=none;
        subgraph cluster_ena1{

            label="";
            style="dashed,rounded";
            color="#440154"            

            bioproject [
                label = "BioProject";
                image = "resources/png/5079138-512.png";
            ];
            biosample [
                label = "BioSample";
                image = "resources/png/5079138-512.png";
            ];
            ena [
                label = "ENA";
                image = "resources/png/5079138-512.png";
            ];
        }
    }

    // Genome engine
    subgraph cluster_ge {
        label = "Genome engine";
        compound = true;
        style=solid;
        color=none;

        subgraph cluster_ge0 {
            label = "";
            compound = true;
            style="dashed,rounded";
            color="#35b779"            ;
        subgraph cluster_ge1 {
            label="";
            style="dashed,rounded";
            color="#31688e"
            ingestion [
                label = "Data ingestion\nservice";
                image = "resources/png/7455241-512.png";

            ];
        }
        subgraph cluster_ge2 {
            label="";
            style="dashed,rounded";
            color="#31688e"
            rank=same;
            backend [
                label = "AToL\nbackend";
                image = "resources/png/7455241-512.png";

            ];
            atoldb [
                label = "AToL\ndatabase";
                image = "resources/png/5079138-512.png";

            ];
        }
        subgraph cluster_ge3 {
            label="";
            style="dashed,rounded";
            color="#31688e"

            engine [
                label = "Genome engine\nlauncher";
                image = "resources/png/7455241-512.png";

            ];
        }
    }
    }

    // Website
    portal [
        label = "AToL web\napplication";
        style="solid,rounded"
        colour=black
        image="resources/png/7604611-512.png"
    ];

    // Pawsey pipelines
    subgraph cluster_pawsey {
        label="HPC";
        compound = true;
        style=solid;
        color=none;
        subgraph cluster_pawsey2 {
            label="";
            style="dashed,rounded";
            color="#440154"    
        pipelines [
            label = "Pipelines";
            image="resources/png/nextflow.png"
        ];
        }
    }

    subgraph cluster_users {
        label="";
        compound = true;
        style=solid;
        color=none;
        user1 [
        label = "Research\ncommunity";
        style="solid,rounded"
        colour=black
        image="resources/png/943339-512.png"
        ];
    }

    // Edges
    bpapm,lab,researcher -> bpadb [ lhead = cluster_dp ];

    bpadb -> new [ label = "New\ndata" ; ltail = cluster_dp; lhead=cluster_dm];
    bpadb -> legacy [ label = "Existing\ndata" ; ltail = cluster_dp; lhead=cluster_dm];

    new,legacy -> broker [
        ltail=cluster_dm;
        // lhead=cluster_broker
        ];

    broker -> bioproject,biosample,ena [
        // ltail = cluster_broker;
        lhead = cluster_ena 
        ];

    biosample, bioproject, ena -> ingestion [ 
        ltail= cluster_ena;
        lhead=cluster_ge
    ];
    ingestion -> backend,atoldb;
    atoldb -> backend[dir=both,constraint=false];
    atoldb -> engine;
    engine -> pipelines;

    backend -> portal;

    portal -> user1 [
        dir=back
    ];

    researcher -> ena [ style = dashed; lhead = cluster_ena ];
}