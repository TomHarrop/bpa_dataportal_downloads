digraph {
    bgcolor = "transparent";
    // margin = 0;
    compound = true;
    splines = false;
    // This attribute applies to the graph itself
    // size = "1,1";
    node [fontname = "lato"; shape="rect"];

    bpa_species [label = "?"; color=transparent];

    subgraph cluster_bpa {
        label = "BPA\ndata portal";
        Package;
        r1 [label="Resource 1"];
        r2 [label="Resource 2"];
        r3 [label="Resource 3"];
        r4 [label="..."];

        d1 [label="Sequence data"];
        d2 [label="More sequence data"];
        d3 [label="Other data"];

    }

    subgraph cluster_rp {
        label = "\"Traditional\"\ngenome project";
        
        Species;
        s1[label="Sample"];
        s2[label="Sample"]
        l1[label="Library"];
        l2[label="Library"];
    }

    // subgraph cluster_BioSample {
    //     label = "BioSample";

    //     dummynode [label=""; color=transparent;];
    // }


    Package:s -> r1:n, r2:n, r3:n,r4:n;
    r1:s -> d1:n;
    r2:s -> d2:n;
    r3:s -> d3:n

    bpa_species -> Species [style = dashed; arrowhead = none;];

    Species:s -> s1:n, s2:n;
    s1:s -> l1:n;
    s2:s -> l2:n;

}